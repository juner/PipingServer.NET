<Project>
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0;netstandard2.1</TargetFramework>
    <IsPackable>true</IsPackable>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/junersproject/PipingServer.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/junersproject/PipingServer.NET.git</RepositoryUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props','$(MSBuildThisFileDirectory)../'))"  />
</Project>